global {

#include <iostream>
#include <algorithm>
#include <vector>
#include <utility>

int main() {
// CHECK: Hello world
  std::cout << "Hello world\n";


// CHECK: 1:1
// CHECK: 2:4
// CHECK: 3:9
  typedef std::pair<int, int> kv_t;
  kv_t dict[] = {{2, 4}, {3, 9}, {1, 1}};

  std::vector<kv_t> sorted(dict, dict+3);
  std::sort(sorted.begin(), sorted.end());

  for (const auto &[v, vv] : sorted)
    std::cout << v << ":" << vv << "\n";

  return 0;
}

